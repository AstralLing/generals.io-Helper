{
	"manifest_version": 3,
	"name": "A generals.io Game Helper",
	"version": "1.2.2",
	"description": "A simple generals.io game helper for chrome.",
  "icons": {
    "32": "./img/32.png",
    "128": "./img/128.png"
  },
	"content_scripts": 
	[
		{
			"matches": ["https://*.generals.io/*", "http://*.generals.io/*"],
			"js": ["main.js"]
		}
	]
}